{
  "name": "liblokinet",
  "version": "0.1.0",
  "description": "liblokinet ffi wrapper",
  "scripts": {
    "clean": "cmake-js clean",
    "dev:compile": "cmake-js build -m -g --debug --CDSTATIC_LINK=ON --CDBUILD_SHARED_LIBS=OFF --CDBUILD_LIBLOKINET=OFF --CDWITH_SYSTEMD=OFF --CDWITH_LTO=OFF --CDNATIVE_BUILD=OFF --CDBUILD_STATIC_DEPS=ON --CDWITH_TESTS=OFF --CDFORCE_OXENMQ_SUBMODULE=ON --CDWITH_BOOTSTRAP=OFF --CDBUILD_DAEMON=OFF",
    "compile": "cmake-js build -m -g --CDSTATIC_LINK=ON --CDBUILD_SHARED_LIBS=OFF --CDBUILD_LIBLOKINET=OFF --CDWITH_SYSTEMD=OFF --CDWITH_LTO=ON --CDNATIVE_BUILD=OFF --CDBUILD_STATIC_DEPS=ON --CDWITH_TESTS=OFF --CDFORCE_OXENMQ_SUBMODULE=ON --CDWITH_BOOTSTRAP=OFF --CDBUILD_DAEMON=OFF",
    "install": "npm run clean && npm run compile",
    "start": "npm run dev:compile && node node/lokinet.js",
    "format": "clang-format -i node/lokinet_ffi.cpp"
  },
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/oxen-io/liblokinet-ffi.git"
  },
  "author": "Oxen Project",
  "license": "GPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/oxen-io/liblokinet-ffi/issues"
  },
  "homepage": "https://github.com/oxen-io/liblokinet-ffi#readme",
  "dependencies": {
    "bindings": "~1.2.1",
    "cmake-js": "^6.2.1",
    "node-addon-api": "^3.0.0",
    "node-fetch": "^3.0.0"
  },
  "cmake-js": {
    "runtime": "electron",
    "runtimeVersion": "14.0.0"
  }
}
